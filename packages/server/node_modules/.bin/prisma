#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules/prisma/build/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules/prisma/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules/prisma/build/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules/prisma/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/prisma@5.7.1/node_modules:/Users/light/Documents/Side_Projects/sports_nine_palace_grid/packages/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prisma/build/index.js" "$@"
else
  exec node  "$basedir/../prisma/build/index.js" "$@"
fi
